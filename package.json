{
  "name": "lenesaile.com",
  "version": "1.1.18",
  "engines": {
    "node": ">=16.x"
  },
  "license": "CC BY-SA",
  "description": "My freelancing website built with Eleventy. It supports three languages and uses my latest workflow, as of autumn 2022.",
  "main": ".eleventy.js",
  "scripts": {
    "clean": "rimraf dist",
    "dev:postcss": "postcss src/assets/css/global.css --o src/_includes/global-inline.css --watch --verbose",
    "dev:scripts": "esbuild src/assets/scripts/theme-toggle.js --watch --outfile=src/_includes/theme-toggle-inline.js",
    "dev:11ty": "eleventy --serve --watch",
    "build:postcss": "NODE_ENV=production postcss src/assets/css/global.css -o src/_includes/global-inline.css",
    "build:scripts": "NODE_ENV=production esbuild src/assets/scripts/theme-toggle.js --bundle --outfile=src/_includes/theme-toggle-inline.js",
    "build:11ty": "cross-env ELEVENTY_ENV=production eleventy",
    "start": "run-p dev:*",
    "build": "run-s clean build:*"
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/madrilene/lenesaile.com.git"
  },
  "author": "Lene Saile",
  "dependencies": {
    "@11ty/eleventy": "^2.0.0",
    "@11ty/eleventy-fetch": "^4.0.0",
    "@11ty/eleventy-img": "^3.0.0",
    "@11ty/eleventy-plugin-bundle": "^1.0.4",
    "@11ty/eleventy-plugin-rss": "^1.2.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
    "gsap": "^3.11.3",
    "tailwindcss": "^3.0.23"
  },
  "devDependencies": {
    "@toycode/markdown-it-class": "^1.2.4",
    "autoprefixer": "^10.4.13",
    "clean-css": "^5.3.1",
    "concurrently": "^8.0.1",
    "cross-env": "^7.0.3",
    "cssnano": "^6.0.0",
    "dayjs": "^1.11.5",
    "dotenv": "^16.0.3",
    "esbuild": "^0.19.0",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "glob": "^10.3.3",
    "html-minifier": "^4.0.0",
    "kleur": "^4.1.5",
    "lodash": "^4.17.21",
    "markdown-it": "^13.0.1",
    "markdown-it-abbr": "^1.0.4",
    "markdown-it-anchor": "^8.6.5",
    "markdown-it-deflist": "^2.1.0",
    "markdown-it-emoji": "^2.0.2",
    "markdown-it-footnote": "^3.0.3",
    "markdown-it-link-attributes": "^4.0.1",
    "markdown-it-mark": "^3.0.1",
    "markdown-it-prism": "^2.3.0",
    "markdown-it-toc-done-right": "^4.2.0",
    "netlify-plugin-cache": "^1.0.3",
    "npm-run-all": "^4.1.5",
    "outdent": "^0.8.0",
    "postcss": "^8.4.8",
    "postcss-cli": "^10.0.0",
    "postcss-csso": "^6.0.1",
    "postcss-custom-media": "^10.0.0",
    "postcss-import": "^15.0.0",
    "postcss-import-ext-glob": "^2.0.1",
    "postcss-js": "^4.0.0",
    "postcss-nesting": "^12.0.1",
    "prettier": "^3.0.1",
    "rimraf": "^5.0.0",
    "sanitize-html": "^2.7.2",
    "slugify": "^1.6.5",
    "speedlify-score": "^3.0.0",
    "terser": "^5.16.1"
  }
}
